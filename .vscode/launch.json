{
    "type": "node",
    "request": "launch",
    "name": "Launch Program",
    "protocol": "inspector",
    "runtimeExecutable": "nodemon",
    "runtimeArgs": [
        "--inspect=4000"
    ],
    "program": "${workspaceFolder}/server/server.js",
    "port": 4000,
    "restart": true,
    "env": {
        "BABEL_DISABLE_CACHE": "1",
        "NODE_ENV": "development"
    },
    "console": "integratedTerminal",
    "internalConsoleOptions": "neverOpen"
}